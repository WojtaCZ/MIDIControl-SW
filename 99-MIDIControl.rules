# MIDIControl udev rules
# type this at the command prompt: sudo cp 99-MIDIControl.rules /etc/udev/rules.d

#
SUBSYSTEM=="usb", ENV{DEVTYPE}=="usb_device", ATTR{idVendor}=="0483", ATTR{idProduct}=="df11", MODE="0666"
SUBSYSTEM=="tty", ATTRS{idVendor}=="6666", ATTRS{idProduct}=="0001", MODE="0666", SYMLINK+="MIDIControl"

